    <string name="http_accept_notifier_ticker">Remote Control Attempt</string>
    <string name="http_accept_notifier_title">Remote Control Attempt</string>
    <string name="http_accept_notifier_text">VNC Automotive Bridge is attempting to control this device.</string>

