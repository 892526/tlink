    <string name="http_accept_title">Accept remote control?</string>
    <string name="http_accept_text">VNC Automotive Bridge is attempting to control this device. Do you want to allow this connection?</string>
    <string name="http_accept_accept">Accept</string>
    <string name="http_accept_reject">Reject</string>

