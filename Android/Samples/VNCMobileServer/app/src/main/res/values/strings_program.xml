    <!-- Messages that can appear on the UI as the server status -->
    <string name="status_keygen">Generating key</string>
    <string name="status_accept_auth">Accepting authentication</string>
    <string name="status_accept_conn">Accepting connection</string>
    <string name="status_deny_auth">Denying authentication</string>
    <string name="status_deny_conn">Denying connection</string>
    <string name="status_supply_auth">Supplying authentication</string>

    <string name="status_authenticating">Authenticating</string>
    <string name="status_requesting_auth">Requesting credentials</string>
    <string name="SS_02_206">Disconnected</string>

    <!-- Messages that can appear in the "secondary status" line -->
    <string name="status2_dialog">To continue, select the VNC Automotive icon in the notification area.</string>

    <!-- Messages that can appear as part of the notifier in the status bar at the top of the screen -->
    <string name="notifier_authreq_text">Select to enter username and password.</string>

    <!-- Other errors that can appear in toast prompts -->
    <string name="SS_03_257">No rendezvous URL configured</string>

    <!-- Authentication (server authenticates viewer) dialog -->
    <string name="auth_dialog_title">Authentication</string>
    <string name="auth_dialog_text">A viewer has provided the following username and password. Do you want to allow it to connect?</string>
    <string name="auth_dialog_username">Username: %s</string>
    <string name="auth_dialog_password">Password: %s</string>
    <string name="auth_dialog_accept">Accept</string>
    <string name="auth_dialog_reject">Reject</string>

    <!-- Reverse authentication (viewer authenticates server) dialog -->
    <string name="authreq_dialog_title">Enter Credentials</string>
    <string name="authreq_dialog_text">The viewer has asked for login details.</string>
    <string name="authreq_dialog_username">Username:</string>
    <string name="authreq_dialog_password">Password:</string>
    <string name="authreq_dialog_ok">Submit</string>

    <!-- Dialog for accepting/rejecting incoming connections -->
    <string name="accept_dialog_title">Accept Connection</string>
    <string name="accept_dialog_text">A viewer at %s is attempting to connect to VNC Automotive Server. Allow the connection?</string>
    <string name="accept_dialog_accept">Accept</string>
    <string name="accept_dialog_reject">Reject</string>

    <!-- Dialog for making outgoing connections -->
    <string name="connect_dialog_title">Connect</string>
    <string name="connect_dialog_text">Enter the address you wish to connect to:</string>
    <string name="connect_dialog_ok">OK</string>
    <string name="connect_dialog_cancel">Cancel</string>

    <!-- Dialog for entering command strings -->
    <string name="cmdstring_dialog_title">Enter Command</string>
    <string name="cmdstring_dialog_text">Enter a VNC Automotive command string:</string>
    <string name="cmdstring_dialog_ok">OK</string>
    <string name="cmdstring_dialog_cancel">Cancel</string>

